{
  "name": "hegentopf/php-assert",
  "description": "A simple assertion library for PHP",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Helmert",
      "email": "assert@hegentopf.de"
    }
  ],
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Hegentopf\\Assert\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hegentopf\\Assert\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit tests --colors=always"
  },
  "config": {
    "platform": {
      "php": "5.6.0"
    }
  }
}
